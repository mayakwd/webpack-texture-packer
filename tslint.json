{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore"
        ],
        "object-literal-sort-keys": false,
        "member-ordering": [true, {
            "order": "statics-first"
        }]
    },

    "rulesDirectory": []
}
